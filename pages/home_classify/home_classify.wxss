#box{
  height: 100vh;
  font-size: 28rpx;
}
.bg_green{
  background:#00ae77;
  color: #fff;
}
.bg_f2{
  background: #f2f2f2;
  color: #8a8a8a;
}
.class_title{
  font-size: 40rpx;
  padding: 50rpx 0 20rpx 0;
}
.class_label{
  display: flex;
  /* justify-content: space-between; */
  flex-wrap: wrap;  
}
.label_text{
  text-align: center;
  display: block;
  width: 22%;
  padding: 10rpx 0;
  border-radius: 100rpx;
  /* margin-right: 40rpx; */
  margin: 30rpx 0 0 20rpx;
}
.footer{
  position: fixed;
  bottom: 5%;
}
.footer button{
  font-size: 34rpx;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border: none;
}
